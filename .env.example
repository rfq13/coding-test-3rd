# OpenAI API Key (Required)
OPENAI_API_KEY=sk-your-api-key-here

# OpenAI Models (Optional - defaults provided)
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Anthropic (Optional)
ANTHROPIC_API_KEY=

# Database (Configured in docker-compose.yml)
DATABASE_URL=postgresql://funduser:fundpass@postgres:5432/funddb

# Redis (Configured in docker-compose.yml)
REDIS_URL=redis://redis:6379/0

# CORS (Optional - override default dev origins)
# Use JSON list format recognized by Pydantic Settings
# Example for local dev:
# ALLOWED_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]
# Example for custom port:
# ALLOWED_ORIGINS=["http://localhost:3002","http://127.0.0.1:3002"]
ALLOWED_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]
